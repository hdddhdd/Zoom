+    const cwd = process.cwd();
+
+    if (config.options.cwd) {
+      utils.log.detail('process root: ' + cwd);
+    }
+
+    config.loaded.map(file => file.replace(cwd, '.')).forEach(file => {
+      utils.log.detail('reading config ' + file);
+    });
