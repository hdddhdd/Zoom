+		// 
+
+		console.log(boxen('foo bar foo bar', {title: 'example', titleAlignment: 'right'}));
+		//  example 
+		// foo bar foo bar
+		// 
+		```
+		*/
+		readonly titleAlignment?: 'left' | 'right' | 'center';
