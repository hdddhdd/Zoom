+		| 'black'
+		| 'red'
+		| 'green'
+		| 'yellow'
+		| 'blue'
+		| 'magenta'
+		| 'cyan'
+		| 'white'
+		| 'gray'
+		| 'grey'
+		| 'blackBright'
+		| 'redBright'
+		| 'greenBright'
+		| 'yellowBright'
+		| 'blueBright'
+		| 'magentaBright'
+		| 'cyanBright'
+		| 'whiteBright',
+		string
+		>;
+
+		/**
+		Style of the box border.
+
+		@default 'single'
+		*/
+		readonly borderStyle?: keyof Boxes | CustomBorderStyle;
+
+		/**
+		Reduce opacity of the border.
+
+		@default false
+		*/
+		readonly dimBorder?: boolean;
+
+		/**
+		Space between the text and box border.
+
+		@default 0
+		*/
+		readonly padding?: number | Spacing;
+
+		/**
+		Space around the box.
+
+		@default 0
+		*/
+		readonly margin?: number | Spacing;
+
+		/**
+		Float the box on the available terminal screen space.
+
+		@default 'left'
+		*/
+		readonly float?: 'left' | 'right' | 'center';
+
+		/**
+		Color of the background.
+		*/
+		readonly backgroundColor?: LiteralUnion<
+		| 'black'
+		| 'red'
+		| 'green'
+		| 'yellow'
+		| 'blue'
+		| 'magenta'
+		| 'cyan'
+		| 'white'
+		| 'blackBright'
+		| 'redBright'
+		| 'greenBright'
+		| 'yellowBright'
+		| 'blueBright'
+		| 'magentaBright'
+		| 'cyanBright'
+		| 'whiteBright',
