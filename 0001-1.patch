+
+run.restart = noop;
+
+bus.on('quit', function onQuit(code) {
+  if (code === undefined) {
+    code = 0;
+  }
+
+  // remove event listener
+  var exitTimer = null;
+  var exit = function () {
+    clearTimeout(exitTimer);
+    exit = noop; // null out in case of race condition
+    child = null;
+    if (!config.required) {
+      // Execute all other quit listeners.
+      bus.listeners('quit').forEach(function (listener) {
+        if (listener !== onQuit) {
+          listener();
+        }
+      });
+      process.exit(code);
