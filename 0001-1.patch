+    if (config.options.dump) {
+      utils.log._log('log', '--------------');
+      utils.log._log('log', 'node: ' + process.version);
+      utils.log._log('log', 'nodemon: ' + version.pinned);
+      utils.log._log('log', 'command: ' + process.argv.join(' '));
+      utils.log._log('log', 'cwd: ' + cwd);
