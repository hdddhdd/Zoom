+      process.stdout._handle.setBlocking(true); // nodejs/node#6456
+    }
+    console.log(help(settings.help));
+    if (!config.required) {
+      process.exit(0);
+    }
+  }
+
+  if (settings.version) {
+    version().then(function (v) {
+      console.log(v);
+      if (!config.required) {
+        process.exit(0);
+      }
+    });
+    return;
+  }
