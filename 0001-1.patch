+  });
+
+  return nodemon;
+}
+
+nodemon.restart = function () {
+  utils.log.status('restarting child process');
+  bus.emit('restart');
+  return nodemon;
+};
+
+nodemon.addListener = nodemon.on = function (event, handler) {
+  if (!eventHandlers[event]) { eventHandlers[event] = []; }
+  eventHandlers[event].push(handler);
