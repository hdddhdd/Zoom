+		topRight: 'b',
+		bottomRight: 'c',
+		bottomLeft: 'd',
+		vertical: 'e',
+		horizontal: 'f'
+	};
+	```
+	*/
+	interface CustomBorderStyle extends BoxStyle {}
+
+	/**
+	Spacing used for `padding` and `margin`.
+	*/
+	interface Spacing {
+		readonly top: number;
+		readonly right: number;
+		readonly bottom: number;
+		readonly left: number;
+	}
+
+	interface Options {
+		/**
+		Color of the box border.
+		*/
